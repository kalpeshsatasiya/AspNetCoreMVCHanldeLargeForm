@model List<int>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <form asp-action="Index" method="post">
        <div class="row">
            @for (int i = 0; i < Model.Count; i++)
            {
                <div class="col">
                    @Html.TextBoxFor(x => Model[i])
                </div>
            }
        </div>
        <div class="row">
            <button class="col-auto btn-primary">Save</button>
        </div>
    </form>
</div>
